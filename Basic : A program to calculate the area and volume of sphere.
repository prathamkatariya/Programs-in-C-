#include <stdio.h> 

int main(){
    // To find the area and volume of sphere 
    float  radius ;
    printf("Enter the radius of sphere \n");
    scanf("%f" , & radius);
    float area = 4*3.14*radius*radius ;
    printf("Area of sphere is %f \n" , area );
    float volume = area*radius/3 ;
    printf("Volume of sphere is %f " , volume );
    return 0;
}
