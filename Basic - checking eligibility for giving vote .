#include <stdio.h> 

int main(){
    // Write a program to check whether a person is eligible for vote or not ?
    int age ;
    printf("Enter your age \n");
    scanf("%d", & age);
    if ( age>=18 )
    {
        printf("you are eligible for vote ");
    }
    else
    {
        printf("you are not eligible for vote ");
    }
    
    return 0;
}
