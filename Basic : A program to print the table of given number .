#include <stdio.h> 

int main(){
    
    // A program to print the table of given number. 
    
    int n ;
    printf("Enter the number to print it's table \n");
    scanf("%d" , & n );
    
    printf("The table of %d is \n" , n);

    for ( int i = 1; i <= 10 ; i++)
    {
        printf("%d \n", n*i );
    }
    
    return 0;
}
